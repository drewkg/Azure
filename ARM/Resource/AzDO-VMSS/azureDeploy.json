{
  "$schema": "https://schema.management.azure.com/schemas/2018-05-01/subscriptionDeploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "Location": {
      "type": "string",
      "defaultValue": "[resourceGroup().location]",
      "metadata": {
        "description": "Location of the resources to be created, defaults to the resource group location."
      }
    },
    "ImageManagementResourceGroup": {
      "type": "string",
      "defaultValue": "AzDO-ImageManagement",
      "metadata": {
        "description": "The Resource Group to hold all resources for Images to be used by AzDO Self Hosted ScaleSet pools."
      }
    },
    "AgentEnvironmentResourceGroup": {
      "type": "string",
      "defaultValue": "AzDO-AgentEnvironment",
      "metadata": {
        "description": "The Resource Group to hold all resources for the AzDO Self Hosted ScaleSet pools."
      }
    }
  },
  "variables": {},
  "resources": [
    {
      "name": "ImageManagementDeployment",
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2020-10-01",
      "location": "[parameters('Location')]",
      "resourceGroup": "[parameters('ImageManagementResourceGroup')]",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "relativePath": "AzDO_ImageManagement/azureDeploy.json"
        },
        "parameters": {
          "Location": {
            "value": "[parameters('Location')]"
          }
        }
      }
    },
    {
      "name": "Windows2019ImageDeployment",
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2020-10-01",
      "location": "[parameters('Location')]",
      "resourceGroup": "[parameters('ImageManagementResourceGroup')]",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "relativePath": "Windows-2019-Base/azureDeploy.json"
        },
        "parameters": {
          "Location": {
            "value": "[parameters('Location')]"
          }
        }
      }
    },
    {
      "name": "AgentEnvironmentDeployment",
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2020-10-01",
      "location": "[parameters('Location')]",
      "resourceGroup": "[parameters('AgentEnvironmentResourceGroup')]",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "relativePath": "AzDO_AgentEnvironment/azureDeploy.json"
        },
        "parameters": {
          "Location": {
            "value": "[parameters('Location')]"
          }
        }
      }
    }
  ]
}